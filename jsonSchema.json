                    "required": [
                        "pocetak",
                        "kraj",
                        "cijena"
                    ],
                    "properties": {
                        "pocetak": {
                            "type": "string",
                            "title": "The pocetak Schema"
                        },
                        "kraj": {
                            "type": "string",
                            "title": "The kraj Schema"
                        },
                        "cijena": {
                            "type": [
