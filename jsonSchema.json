            },
            "lokacija": {
                "type": "object",
                "title": "The lokacija Schema",
                "required": [
                    "idlokacije",
                    "opislokacije"
                ],
                "properties": {
                    "idlokacije": {
                        "type": "integer",
                        "title": "The idlokacije Schema"
                    },
                    "opislokacije": {
                        "type": "string",
                        "title": "The opislokacije Schema"
                    }
                }
