{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "title": "A Schema",
        "required": [
            "idGaraza",
            "imeGaraza",
            "ulica",
            "broj",
            "kvart",
            "brojMjesta",
            "brojRazina",
            "maksimalnaVisina",
            "dostupnostPovlasteneKarte",
            "tarife",
            "lokacija"
        ],
        "properties": {
            "idGaraza": {
                "type": "integer",
                "title": "The idGaraza Schema"
            },
            "imeGaraza": {
                "type": "string",
                "title": "The imeGaraza Schema"
            },
            "ulica": {
                "type": "string",
                "title": "The ulica Schema"
            },
            "broj": {
                "type": "string",
                "title": "The broj Schema"
            },
            "kvart": {
                "type": "string",
                "title": "The kvart Schema"
            },
            "brojMjesta": {
                "type": "integer",
                "title": "The brojMjesta Schema"
            },
            "brojRazina": {
                "type": "integer",
                "title": "The brojRazina Schema"
            },
            "maksimalnaVisina": {
                "type": [
                    "number",
                    "integer"
                ],
                "title": "The maksimalnaVisina Schema"
            },
            "dostupnostPovlasteneKarte": {
                "type": "boolean",
                "title": "The dostupnostPovlasteneKarte Schema"
            },
            "tarife": {
                "type": "array",
                "title": "The tarife Schema",
                "items": {
                    "type": "object",
