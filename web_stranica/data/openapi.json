                            }
                        }
                    }
                }
            }
        },
        "/api/specifikacija": {
            "get": {
                "summary": "Get garage specification",
                "description": "Fetch the specification of a garage.",
                "responses": {
                    "200": {
                        "description": "Garage specification found.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Garaza"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Garaza": {
                "type": "object",
                "properties": {
                    "idgaraza": {
                        "type": "integer"
                    },
                    "imegaraza": {
                        "type": "string"
                    },
                    "ulica": {
                        "type": "string"
                    },
                    "broj": {
                        "type": "string"
                    },
                    "kvart": {
                        "type": "string"
                    },
                    "brojmjesta": {
                        "type": "integer"
                    },
                    "brojrazina": {
                        "type": "integer"
                    },
                    "maksimalnavisina": {
                        "type": "number"
                    },
                    "dostupnostpovlastenekarte": {
                        "type": "boolean"
                    },
                    "tarife":
                    {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "pocetak": {
                                    "type": "string",
                                    "format": "time"
                                },
                                "kraj": {
                                    "type": "string",
                                    "format": "time"
                                },
                                "cijena": {
                                    "type": "number"
                                }
                            },
                            "required": ["pocetak", "kraj", "cijena"]
                        }
                    },
                    "lokacija": {
                        "type": "object",
                        "properties": {
                            "idlokacije": {
                                "type": "integer"
                            },
                            "opislokacije": {
                                "type": "string"
                            }
                        },
                        "required": ["idlokacije", "opislokacije"]
                    }
                }
            },
            "GarazaPost": {
                "type": "object",
                "properties": {
                    "imegaraza": {
                        "type": "string"
                    },
                    "ulica": {
                        "type": "string"
                    },
                    "broj": {
                        "type": "string"
                    },
                    "kvart": {
                        "type": "string"
                    },
                    "brojmjesta": {
                        "type": "integer"
                    },
                    "brojrazina": {
                        "type": "integer"
                    },
                    "maksimalnavisina": {
                        "type": "number"
                    },
                    "dostupnostpovlastenekarte": {
                        "type": "boolean"
                    },
                    "lokacija": {
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "object",
                                "properties": {
                                "idlokacije": {
                                    "type": "integer"
                                },
                                "opislokacije": {
                                    "type": "string"
                                }
                                },
                                "required": ["idlokacije", "opislokacije"]
                            }
                        ]
                    }
                }
            },
            "GarazaResponse": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "response": {
                        "type": "object",
                        "properties": {
                            "data": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/Garaza"
                                }
                            }
                        }
                    }
                }
            },
            "ErrorResponse": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "response": {
                        "type": "string",
                        "nullable": true
                    }
                }
            }
        }
    }
}