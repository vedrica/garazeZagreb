        "/api/tipLokacije/{idLokacije}": {
            "get": {
                "summary": "Get garages by location",
                "description": "Fetch garages by location.",
                "parameters": [
                    {
                        "name": "idLokacije",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Garages found.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GarazaResponse"
                                }
