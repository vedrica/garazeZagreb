        "/api/name": {
            "get": {
                "summary": "Get garage by name",
                "description": "Fetch a garage by name.",
                "parameters": [
                    {
                        "name": "value",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "Garage name, case sensitive."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Garage found.",
                        "content": {
                            "application/json": {
