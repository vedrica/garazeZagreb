                    }
                }
            },
            "delete": {
                "summary": "Delete garage by ID",
                "description": "Delete a garage by ID.",
                "parameters": [
                    {
                        "name": "idGaraze",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Garage deleted.",
                        "content": {
                            "application/json": {
